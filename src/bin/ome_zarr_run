import argparse

parser = argparse.ArgumentParser()

subparsers = parser.add_subparsers()

maxproject = subparsers.add_parser('project')
maxproject.add_argument('--projection_type', default = 'max')
maxproject.add_argument('--along', default = 'z')
maxproject.add_argument('--output_name', default = None)
maxproject.add_argument('--resolutions', default = 'all')
maxproject.add_argument('--label_name', default = None)

if __name__ == '__main__':
    print('DOC')